<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.globlehiphilemon.s2_recyclerview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service
            android:name=".servicethread.ServiceActivity"
            android:enabled="true"
            android:exported="true"></service>

        <activity android:name=".servicethread.ThreadActivity" />
        <activity android:name=".sqlite.RecyclerEtudiantActivity" />
        <activity android:name=".sqlite.SqliteActivity" />
        <activity android:name=".PreferenceActivity" />
        <activity android:name=".IntentActivity" />
        <activity android:name=".RecyclerActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>